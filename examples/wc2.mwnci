// A simple script to read a file and count the characters.
//
//   $ ./mwnci ./wc2.mwnci
//
//   Read 16 lines
//
// That matches :)
//
//   $ wc -l ./wc2.mwnci
//   16 ./wc2.mwnci
//

// Open the file
let fh = open( "./wc2.mwnci", "r" );
lines = fh.lines();
print( "Read ", len(lines), " lines\n" );
