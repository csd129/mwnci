    function object.isalpha() {return isalpha(self)}
    function object.len() {return len(self)}
    function object.oct2bin() {return oct2bin(self)}
    function object.oct2dec() {return oct2dec(self)}
    function object.oct2hex() {return oct2hex(self)}
    function object.reverse() {return reverse(self)}
